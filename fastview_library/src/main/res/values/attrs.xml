<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- TagFlowLayout 自定义属性 -->
    <attr name="isAssaginSpace" format="boolean"></attr>
    <declare-styleable name="TagFlowLayout">
        <attr name="isAssaginSpace" />
    </declare-styleable>

    <declare-styleable name="CenterViewGroup">
        <attr name="cv_numberColumn" format="integer" />
        <!-- 4,3,2,1..类似这样传递每一行的个数-->
        <attr name="cv_numberColumnArray" format="string" />
        <!--  初值方向的间距-->
        <attr name="cv_verticalSpacing" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ClearAutoCompleteText">
        <attr name="clear_tint" format="color" />
    </declare-styleable>
    <declare-styleable name="SideBar">
        <attr name="android:textSize" />
        <attr name="android:textColor" />
        <attr name="android:background" />
        <attr name="android:text" />
        <!-- 设置字母导航栏,字母被选中后的背景 -->
        <attr name="sb_touch_character_drawable" format="reference" />
        <!-- 没有用到的导航字母的颜色,这里可以额外设定的 -->
        <attr name="validCharacterTextColor" format="color" />
    </declare-styleable>
</resources>